<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0"
    xmlns:e="http://ant.apache.org/ivy/extra"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">
    <info organisation="org.sosy_lab" module="javasmt-solver-z3">
        <license name="MIT License" url="https://opensource.org/licenses/MIT"/>
        <description>
            Z3 solver and JavaSMT JNI wrapper.
            Z3 is provided under the MIT License.
        </description>
    </info>

    <configurations>
        <conf name="solver-z3" />
    </configurations>

    <publications defaultconf="solver-z3">
        <artifact name="libz3" conf="solver-z3" type="shared-object" ext="so" />
        <artifact name="libz3j" conf="solver-z3" type="shared-object" ext="so"/>
    </publications>

    <dependencies></dependencies>

</ivy-module>
